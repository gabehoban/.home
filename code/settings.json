{
  "code-runner.runInTerminal": true,
  "editor.fontSize": 15,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.largeFileOptimizations": false,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "none",
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.trimAutoWhitespace": true,
  "editor.wordWrap": "on",
  "files.autoSave": "onFocusChange",
  "files.associations": {
    "Tapfile*": "ruby",
    "Caskfile*": "ruby"
  },
  "files.insertFinalNewline": true,
  "java.errors.incompleteClasspath.severity": "ignore",
  "prettier.singleQuote": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontFamily": "Hack Nerd Font",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.rendererType": "dom",
  "workbench.enableExperiments": false,
  "zenMode.hideTabs": false,
  "window.titleBarStyle": "native",
  "workbench.startupEditor": "newUntitledFile",
  "cSpell.userWords": [
    "Balrog",
    "COUCHDB",
    "Cyberdeamon",
    "Cyberdemon",
    "Datepicker",
    "Fastify",
    "Gabe",
    "Hoban",
    "Kirki",
    "MMDDYYYY",
    "Probility",
    "Restrictor",
    "Toastr",
    "Westgate",
    "Xcode",
    "Ypsi",
    "addto",
    "autoload",
    "blipp",
    "ciph",
    "cocoapods",
    "cout",
    "crossorigin",
    "datauristring",
    "dataurl",
    "dataurlnewwindow",
    "dragula",
    "echarts",
    "formpicker",
    "gabe",
    "gabehoban",
    "gigasecond",
    "golang",
    "gonum",
    "hitpoints",
    "hobang",
    "homebrew",
    "iphonesimulator",
    "jspdf",
    "keybase",
    "makeid",
    "minlength",
    "pipefail",
    "queryselector",
    "scode",
    "sshman",
    "stylelint",
    "templist",
    "tfcoreml",
    "uploader",
    "v",
    "xcodebuild",
    "xcpretty",
    "xcworkspace",
    "youtube"
  ],
  "javascript.format.insertSpaceAfterConstructor": true,
  "editor.wrappingIndent": "deepIndent",
  "java.codeGeneration.generateComments": true,
  "java.codeGeneration.hashCodeEquals.useInstanceof": true,
  "java.codeGeneration.toString.skipNullValues": true,
  "java.codeGeneration.useBlocks": true,
  "extensions.ignoreRecommendations": true,
  "editor.codeActionsOnSave": {
    // For ESLint
    "source.fixAll.eslint": true,
    // For TSLint
    "source.fixAll.tslint": true,
    // For Stylelint
    "source.fixAll.stylelint": true
  },
  "zenMode.fullScreen": false,
  "workbench.colorCustomizations": {
    "[One Dark Pro (Italic)]": {
      "editor.background": "#282c34"
    },
    "[Palenight Theme]": {
      "editorOverviewRuler.addedForeground": "#0000",
      "editorOverviewRuler.modifiedForeground": "#0000",
      "editorOverviewRuler.deletedForeground": "#0000",
      "editorOverviewRuler.border": "#0000",
      "editorOverviewRuler.wordHighlightForeground": "#0000",
      "editorOverviewRuler.errorForeground": "#0000",
      "editorOverviewRuler.warningForeground": "#0000",
      "editorOverviewRuler.infoForeground": "#0000",
      "editorOverviewRuler.selectionHighlightForeground": "#f000",
      "editor.selectionHighlightBorder": "#f000",
      "editorGroupHeader.tabsBackground": "#31364A",
      "tab.border": "#31364A",
      "editorGroupHeader.noTabsBackground": "#292D3E",
      "activityBar.background": "#292D3E",
      "activityBar.border": "#292D3E",
      "titleBar.activeBackground": "#292D3E",
      "titleBar.inactiveBackground": "#292D3E"
    },
    "[Field Lights]": {
      "titleBar.activeBackground": "#212733",
      "titleBar.inactiveBackground": "#212733",
      "titleBar.border": "#212733"
    },
    "[Forest Night]": {
      "editorGroupHeader.noTabsBackground": "#3c4c55"
    }
  },
  "workbench.activityBar.visible": true,
  "workbench.sideBar.location": "left",
  "window.title": "",
  "editor.cursorSmoothCaretAnimation": true,
  "zenMode.centerLayout": false,
  "zenMode.restore": true,
  "breadcrumbs.filePath": "off",
  "explorer.confirmDragAndDrop": false,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment.line.double-slash",
          "comment",
          "string.quoted",
          "storage.type.function.arrow.js",
          "variable.language.default.js",
          "variable.other.module.js",
          "keyword.operator.relational.js"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
      //    {
      //        "scope": "variable.other.module.js",
      //        "settings": {
      //            "foreground": "#bfc7d5ff",
      //        }
      //    }
    ]
  },
  "breadcrumbs.enabled": false,
  "workbench.colorTheme": "Andromeda Bordered",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "explorer.confirmDelete": false,
  "code-runner.saveAllFilesBeforeRun": true,
  "editor.fontFamily": "'JetBrainsMono Nerd Font Mono'",
  "editor.fontLigatures": true,
  "workbench.fontAliasing": "auto",
  "editor.lineHeight": 22,
  "editor.letterSpacing": 0.5,
  "editor.fontWeight": "400",
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 5,
  "editor.cursorBlinking": "solid",
  "highlight-matching-tag.styles": {
    "opening": {
      "left": {
        "custom": {
          "borderWidth": "0 0 0 1.5px",
          "borderStyle": "solid",
          "borderColor": "yellow",
          "borderRadius": "5px",
          "overviewRulerColor": "white"
        }
      },
      "right": {
        "custom": {
          "borderWidth": "0 1.5px 0 0",
          "borderStyle": "solid",
          "borderColor": "yellow",
          "borderRadius": "5px",
          "overviewRulerColor": "white"
        }
      }
    }
  },
  "go.formatTool": "goimports",
  "python.jediEnabled": false,
  "python.insidersChannel": "off",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.zoomLevel": 1,
  "go.useLanguageServer": true,
  "[go]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    // Optional: Disable snippets, as they conflict with completion ranking.
    "editor.snippetSuggestions": "none"
  },
  "[go.mod]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "gopls": {
    // Add parameter placeholders when completing a function.
    "usePlaceholders": true,
    // If true, enable additional analyses with staticcheck.
    // Warning: This will significantly increase memory usage.
    "staticcheck": false
  },
  "workbench.iconTheme": "material-icon-theme",
  "editor.rulers": [
    79
  ],
  "editor.roundedSelection": false,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
  "code-runner.fileDirectoryAsCwd": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "psi-header.changes-tracking": {
    "autoHeader": "autoSave",
    "isActive": true,
    "enforceHeader": false,
    "modAuthor": "Modified By: ",
    "modDate": "Last Modified: ",
    "modDateFormat": "date",
    "include": [],
    "exclude": [
      "markdown",
      "json"
    ],
  },
  "psi-header.config": {
    "forceToTop": true,
    "blankLinesAfter": 0,
    "license": "GPL-3.0",
    "company": "Gabriel Hoban",
  },
  "psi-header.lang-config": [
    {
      "language": "lua",
      "begin": "--[[",
      "prefix": "--",
      "end": "--]]",
      "blankLinesAfter": 0
    },
    {
      "language": "python",
      "begin": "###",
      "prefix": "# ",
      "end": "###",
      "blankLinesAfter": 0,
      "beforeHeader": [
        "#!/usr/bin/env python3",
        "# -*- coding:utf-8 -*-"
      ]
    },
    {
      "language": "javascript",
      "begin": "/**",
      "prefix": " * ",
      "end": " */",
      "blankLinesAfter": 0,
      "forceToTop": true
    },
    {
      "language": "scss",
      "begin": "/*",
      "prefix": " * ",
      "end": " */",
      "blankLinesAfter": 0,
      "forceToTop": true
    },
    {
      "language": "html",
      "begin": "<!--",
      "prefix": " ~ ",
      "end": "-->",
      "blankLinesAfter": 0,
      "forceToTop": true
    },
    {
      "language": "typescript",
      "mapTo": "javascript"
    }
  ],
  "psi-header.templates": [
    {
      "language": "*",
      "template": [
        "Copyright (c) <<year>> <<company>>. All Rights Reserved.",
        "",
        "@file <<filename>> (<<projectname>>)",
        "@license <<spdxid>> <<licensename>>",
      ]
    },
    {
      "language": "typescript",
      "mapTo": "javascript"
    }
  ],
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[scss]": {
    "editor.defaultFormatter": "numso.prettier-standard-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.renameOnType": true,
  "[javascript]": {
    "editor.defaultFormatter": "numso.prettier-standard-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "git.confirmSync": false,
  "git.alwaysSignOff": true,
  "git.enableCommitSigning": true,
  "git.enableSmartCommit": true,
  "git.supportCancellation": true,
  "gitmoji.autoCommit": true,
  "gitmoji.autoClose": true,
  "workbench.preferredDarkColorTheme": "Palenight Operator",
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "yaml.schemas": {
    "file:///toc.schema.json": "/toc\\.yml/i"
  },
"angular.ngdk": "",
"python.languageServer": "Microsoft",
}
